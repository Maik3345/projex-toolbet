{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["prettier"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "no-undef": "off",
    "no-unused-vars": "off"
  },
  "env": {
    "node": true,
    "es2022": true
  }
}
